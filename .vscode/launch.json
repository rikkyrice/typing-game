    "version": "0.2.0",
    "configurations": [
      {
        "name": "Launch",
        "type": "go",
        "request": "launch",
        "mode": "debug",
        "program": "${workspaceFolder}/api/main.go",
        "env": {},
        "args": [],
        "showLog": true
      },
      {
        "type": "firefox",
        "request": "launch",
        "name": "vuejs: firefox",
        "url": "https://localhost:7070",
        "webRoot": "${workspaceFolder}/src",
        "pathMappings": [
          {
            "url": "webpack:///src",
            "path": "${workspaceFolder}/frontend/src"
          },
          {
            "url": "webpack:///src",
            "path": "${workspaceFolder}/frontend/src"
          },
          {
            "url": "webpack:///src/",
            "path": "${webRoot}/"
          }
        ]
      }
    ]
}